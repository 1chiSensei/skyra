{
  "name": "skyra",
  "version": "5.0.0",
  "description": "Multipurpose Discord Bot built on Klasa",
  "main": "dist/src/skyra.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json src/**/*.ts --fix",
    "pm2:delete": "pm2 delete Skyra",
    "pm2:logs": "pm2 logs Skyra --lines=200",
    "pm2:restart": "pm2 restart Skyra",
    "pm2:start-harmony": "pm2 start dist/src/Skyra.js --node-args=\"--harmony\" --name=\"Skyra\"",
    "pm2:start": "pm2 start dist/src/Skyra.js --name=\"Skyra\"",
    "pm2:stop": "pm2 stop Skyra",
    "start": "node dist/src/Skyra.js",
    "test": "tslint -p tsconfig.json src/**/*.ts",
    "update": "yarn",
    "compile": "tsc -p .",
    "watch": "tsc -p . -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyranet/Skyra.git"
  },
  "keywords": [
    "discord.js",
    "klasa",
    "bot",
    "standalone"
  ],
  "contributors": [
    "kyranet"
  ],
  "private": true,
  "author": "kyranet <kyradiscord@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kyranet/Skyra/issues"
  },
  "dependencies": {
    "canvas-constructor": "^2.0.1",
    "canvas": "^2.2.0",
    "diff": "^3.5.0",
    "discord.js": "github:discordjs/discord.js",
    "fs-nextra": "^0.3.7",
    "gifencoder": "^1.1.0",
    "heapdump": "^0.3.11",
    "klasa": "dirigeants/klasa#settings",
    "node-fetch": "^2.3.0",
    "rethinkdb-ts": "^2.4.0-rc.11",
    "typescript": "^3.2.2",
    "veza": "^0.3.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "@types/node-fetch": "^2.1.4",
    "eslint": "^5.10.0",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "tslint": "^5.11.0"
  }
}
